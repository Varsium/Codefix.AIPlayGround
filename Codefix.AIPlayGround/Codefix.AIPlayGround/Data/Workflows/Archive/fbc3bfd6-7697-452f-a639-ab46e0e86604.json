      "Label": "Available Items",
      "Type": "default",
      "Properties": {},
      "IsSelected": false,
      "From": "inventory-check",
      "To": "fulfillment-center"
    },
    {
      "Id": "conn-3",
      "FromNodeId": "fulfillment-center",
      "ToNodeId": "shipping-service",
      "FromPort": "shipped",
      "ToPort": "package",
      "Label": "Package Info",
      "Type": "default",
      "Properties": {},
      "IsSelected": false,
      "From": "fulfillment-center",
      "To": "shipping-service"
    },
    {
      "Id": "conn-4",
      "FromNodeId": "shipping-service",
      "ToNodeId": "notification-service",
      "FromPort": "tracking",
      "ToPort": "event",
      "Label": "Tracking Update",
      "Type": "default",
      "Properties": {},
      "IsSelected": false,
      "From": "shipping-service",
      "To": "notification-service"
    }
